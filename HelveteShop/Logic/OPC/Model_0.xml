<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="OPCUAModelDesign.xslt"?>
<ModelDesign xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" TargetNamespace="http://opcfoundation.org/UA/" xmlns="http://opcfoundation.org/UA/ModelDesign.xsd">
  <Namespaces>
    <Namespace Name="ua" Prefix="Dependencies" XmlPrefix="ua">http://opcfoundation.org/UA/</Namespace>
    <Namespace Name="Helvete" Prefix="Dependencies" XmlPrefix="Helvete">http://Helvete.org/UA/</Namespace>
  </Namespaces>
  <ObjectType xmlns:q1="http://Helvete.org/UA/" SymbolicName="q1:SClient" xmlns:q2="http://opcfoundation.org/UA/" BaseType="q2:BaseObjectType">
    <BrowseName />
    <Children>
      <Variable SymbolicName="q1:ID" TypeDefinition="q2:BaseVariableType" ModellingRule="None" DataType="q2:Int32" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>ID</BrowseName>
      </Variable>
      <Variable SymbolicName="q1:name" TypeDefinition="q2:BaseVariableType" ModellingRule="None" DataType="q2:String" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>name</BrowseName>
      </Variable>
      <Variable SymbolicName="q1:address" TypeDefinition="q2:BaseVariableType" ModellingRule="None" DataType="q2:String" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>address</BrowseName>
      </Variable>
    </Children>
  </ObjectType>
  <ObjectType xmlns:q3="http://Helvete.org/UA/" SymbolicName="q3:SVinyl" xmlns:q4="http://opcfoundation.org/UA/" BaseType="q4:BaseObjectType">
    <BrowseName />
    <Children>
      <Variable SymbolicName="q3:ID" TypeDefinition="q4:BaseVariableType" ModellingRule="None" DataType="q4:Int32" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>ID</BrowseName>
      </Variable>
      <Variable SymbolicName="q3:Title" TypeDefinition="q4:BaseVariableType" ModellingRule="None" DataType="q4:String" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>Title</BrowseName>
      </Variable>
      <Variable SymbolicName="q3:Band" TypeDefinition="q4:BaseVariableType" ModellingRule="None" DataType="q4:String" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>Band</BrowseName>
      </Variable>
      <Variable SymbolicName="q3:Price" TypeDefinition="q4:BaseVariableType" ModellingRule="None" DataType="q4:Decimal" ValueRank="Scalar" AccessLevel="ReadWrite" Historizing="false">
        <BrowseName>Price</BrowseName>
      </Variable>
      <Variable SymbolicName="q3:InStock" TypeDefinition="q4:BaseVariableType" ModellingRule="None" DataType="q4:Int32" ValueRank="Scalar" AccessLevel="Write" Historizing="false">
        <BrowseName>InStock</BrowseName>
      </Variable>
    </Children>
  </ObjectType>
  <ObjectType xmlns:q5="http://Helvete.org/UA/" SymbolicName="q5:SOrder" xmlns:q6="http://opcfoundation.org/UA/" BaseType="q6:BaseObjectType">
    <Children>
      <Variable SymbolicName="q5:Vinyl" TypeDefinition="q6:BaseVariableType" ModellingRule="None" DataType="q6:String" ValueRank="Scalar" AccessLevel="ReadWrite" />
      <Variable SymbolicName="q5:ID" ModellingRule="None" DataType="q6:Int32" ValueRank="Scalar" Historizing="false" />
      <Variable SymbolicName="q5:Client" ModellingRule="None" DataType="q6:String" ValueRank="Scalar" Historizing="false" />
    </Children>
  </ObjectType>
</ModelDesign>